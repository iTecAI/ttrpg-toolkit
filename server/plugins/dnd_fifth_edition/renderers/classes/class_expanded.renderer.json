{
    "supertype": "render",
    "type": "group",
    "children": [
        {
            "supertype": "render",
            "type": "stack",
            "direction": "horizontal",
            "spacing": 2,
            "children": [
                {
                    "supertype": "render",
                    "type": "stack",
                    "direction": "vertical",
                    "spacing": 2,
                    "children": [
                        {
                            "supertype": "render",
                            "type": "card",
                            "title": {
                                "title": "Save Proficiencies",
                                "icon": {
                                    "type": "icon",
                                    "icon": "md.MdShield"
                                }
                            },
                            "children": {
                                "supertype": "source",
                                "type": "list",
                                "source": "save_proficiency",
                                "renderer": {
                                    "supertype": "render",
                                    "type": "chip",
                                    "filled": true,
                                    "text": {
                                        "supertype": "value",
                                        "type": "function",
                                        "function": "$file:functions/capitalize.js:lines",
                                        "opts": {
                                            "name": "$self"
                                        }
                                    }
                                }
                            }
                        },
                        {
                            "supertype": "render",
                            "type": "group",
                            "children": {
                                "supertype": "source",
                                "type": "generator",
                                "function": {
                                    "function": "$file:functions/parse_skill_array.js:lines",
                                    "opts": {
                                        "skills": "$data:skill_proficiency"
                                    }
                                },
                                "renderer": {
                                    "supertype": "render",
                                    "type": "card",
                                    "title": {
                                        "title": "Skill Proficiencies",
                                        "icon": {
                                            "type": "icon",
                                            "icon": "md.MdStar"
                                        },
                                        "subtitle": "$data:sub"
                                    },
                                    "children": {
                                        "supertype": "source",
                                        "type": "list",
                                        "source": "choices",
                                        "renderer": {
                                            "supertype": "render",
                                            "type": "chip",
                                            "filled": true,
                                            "text": "$self"
                                        }
                                    }
                                }
                            }
                        }
                    ]
                },
                {
                    "supertype": "render",
                    "type": "stack",
                    "direction": "vertical",
                    "spacing": 2,
                    "children": [
                        {
                            "supertype": "render",
                            "type": "group",
                            "conditionalRender": {
                                "function": "(opts) => Boolean(opts.item)",
                                "opts": {
                                    "item": "$data:multiclass_skill_proficiency"
                                }
                            },
                            "children": {
                                "supertype": "source",
                                "type": "generator",
                                "function": {
                                    "function": "$file:functions/parse_skill_array.js:lines",
                                    "opts": {
                                        "skills": "$data:multiclass_skill_proficiency"
                                    }
                                },
                                "renderer": {
                                    "supertype": "render",
                                    "type": "card",
                                    "title": {
                                        "title": "Multiclass Skill Proficiencies",
                                        "icon": {
                                            "type": "icon",
                                            "icon": "md.MdStar"
                                        },
                                        "subtitle": "$data:sub"
                                    },
                                    "children": {
                                        "supertype": "source",
                                        "type": "list",
                                        "source": "choices",
                                        "renderer": {
                                            "supertype": "render",
                                            "type": "chip",
                                            "filled": true,
                                            "text": "$self"
                                        }
                                    }
                                }
                            }
                        }
                    ]
                }
            ]
        }
    ]
}
