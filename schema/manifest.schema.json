{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$ref": "#/definitions/PluginManifest",
  "definitions": {
    "PluginManifest": {
      "type": "object",
      "properties": {
        "plugin_data": {
          "$ref": "#/definitions/PluginDataModel"
        },
        "entrypoints": {
          "type": "object",
          "additionalProperties": {
            "$ref": "#/definitions/EntrypointModel"
          }
        },
        "dice": {
          "$ref": "#/definitions/DiceModel"
        },
        "data_source": {
          "$ref": "#/definitions/DataSource"
        },
        "assets": {
          "type": "object",
          "additionalProperties": {
            "type": "string"
          }
        },
        "documentTypes": {
          "type": "object",
          "additionalProperties": {
            "$ref": "#/definitions/DocumentType"
          }
        }
      },
      "required": [
        "plugin_data",
        "entrypoints"
      ],
      "additionalProperties": false
    },
    "PluginDataModel": {
      "type": "object",
      "properties": {
        "slug": {
          "type": "string"
        },
        "display_name": {
          "type": "string"
        },
        "tags": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/PluginTag"
          }
        },
        "libraries": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "dependencies": {
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      },
      "required": [
        "slug",
        "display_name",
        "tags"
      ],
      "additionalProperties": false
    },
    "PluginTag": {
      "type": "string",
      "enum": [
        "system",
        "dice",
        "character_sheet",
        "character_loader",
        "npc_source",
        "npc_builder",
        "documents",
        "asset_pack",
        "data_source"
      ]
    },
    "EntrypointModel": {
      "type": "object",
      "properties": {
        "file": {
          "type": "string"
        },
        "controllers": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "exports": {
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      },
      "required": [
        "file"
      ],
      "additionalProperties": false
    },
    "DiceModel": {
      "type": "object",
      "properties": {
        "types": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "roll_implementation": {
          "$ref": "#/definitions/RollImplementationModel"
        },
        "implements_object": {
          "type": "boolean"
        },
        "implements_string": {
          "type": "boolean"
        },
        "reroll_operations": {
          "type": "object",
          "additionalProperties": {
            "type": "string"
          }
        }
      },
      "required": [
        "types",
        "roll_implementation",
        "reroll_operations"
      ],
      "additionalProperties": false
    },
    "RollImplementationModel": {
      "anyOf": [
        {
          "type": "object",
          "properties": {
            "type": {
              "type": "string",
              "const": "integer"
            },
            "type_map": {
              "type": "object",
              "additionalProperties": {
                "type": "array",
                "items": {
                  "type": "number"
                }
              }
            }
          },
          "required": [
            "type",
            "type_map"
          ],
          "additionalProperties": false
        },
        {
          "type": "object",
          "properties": {
            "type": {
              "type": "string",
              "const": "selection"
            },
            "type_map": {
              "type": "object",
              "additionalProperties": {
                "type": "array",
                "items": {
                  "type": [
                    "number",
                    "string"
                  ]
                }
              }
            }
          },
          "required": [
            "type",
            "type_map"
          ],
          "additionalProperties": false
        }
      ]
    },
    "DataSource": {
      "type": "object",
      "properties": {
        "kwargs": {
          "type": "object"
        },
        "default_category": {
          "type": "string"
        },
        "categories": {
          "type": "object",
          "additionalProperties": {
            "$ref": "#/definitions/DataSourceCategory"
          }
        }
      },
      "required": [
        "kwargs",
        "default_category",
        "categories"
      ],
      "additionalProperties": false
    },
    "DataSourceCategory": {
      "type": "object",
      "properties": {
        "display_name": {
          "type": "string"
        },
        "search_fields": {
          "type": "object",
          "additionalProperties": {
            "$ref": "#/definitions/SearchField"
          }
        },
        "files": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "renderer": {
          "$ref": "#/definitions/CompendiumItem"
        }
      },
      "required": [
        "display_name",
        "search_fields",
        "files",
        "renderer"
      ],
      "additionalProperties": false
    },
    "SearchField": {
      "type": "object",
      "properties": {
        "field": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "array",
              "items": {
                "type": "string"
              }
            }
          ]
        },
        "type": {
          "type": "string",
          "enum": [
            "string",
            "number",
            "boolean",
            "select"
          ]
        },
        "label": {
          "type": "string"
        }
      },
      "required": [
        "field",
        "type",
        "label"
      ],
      "additionalProperties": false
    },
    "CompendiumItem": {
      "type": "object",
      "properties": {
        "supertype": {
          "type": "string",
          "const": "compendium-item"
        },
        "type": {
          "type": "string"
        },
        "plugin": {
          "type": "string"
        },
        "slug": {
          "$ref": "#/definitions/ValueItem"
        },
        "displayName": {
          "$ref": "#/definitions/ValueItem"
        },
        "source": {
          "$ref": "#/definitions/ValueItem"
        },
        "avatar": {
          "$ref": "#/definitions/AvatarType"
        },
        "media": {
          "type": "object",
          "properties": {
            "source": {
              "$ref": "#/definitions/ValueItem"
            },
            "alt": {
              "$ref": "#/definitions/ValueItem"
            }
          },
          "required": [
            "source",
            "alt"
          ],
          "additionalProperties": false
        },
        "briefContents": {
          "anyOf": [
            {
              "type": "array",
              "items": {
                "$ref": "#/definitions/AllRenderItems"
              }
            },
            {
              "$ref": "#/definitions/AllSourceItems"
            }
          ]
        },
        "fullContents": {
          "$ref": "#/definitions/AllRenderItems"
        }
      },
      "required": [
        "supertype",
        "type",
        "plugin",
        "slug",
        "displayName",
        "source",
        "briefContents",
        "fullContents"
      ],
      "additionalProperties": false
    },
    "ValueItem": {
      "anyOf": [
        {
          "$ref": "#/definitions/Literal"
        },
        {
          "$ref": "#/definitions/TextItem"
        },
        {
          "$ref": "#/definitions/FunctionalItem"
        },
        {
          "$ref": "#/definitions/DataItem"
        },
        {
          "$ref": "#/definitions/FormDataItem"
        }
      ]
    },
    "Literal": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "number"
        },
        {
          "type": "boolean"
        },
        {
          "type": "null"
        },
        {
          "not": {}
        }
      ]
    },
    "TextItem": {
      "type": "object",
      "properties": {
        "supertype": {
          "type": "string",
          "const": "value"
        },
        "type": {
          "type": "string",
          "const": "text"
        },
        "content": {
          "type": "string"
        },
        "substitutions": {
          "type": "object",
          "additionalProperties": {
            "$ref": "#/definitions/ValueItem"
          }
        }
      },
      "required": [
        "supertype",
        "type",
        "content",
        "substitutions"
      ],
      "additionalProperties": false
    },
    "FunctionalItem": {
      "type": "object",
      "properties": {
        "supertype": {
          "type": "string",
          "const": "value"
        },
        "type": {
          "type": "string",
          "const": "function"
        },
        "function": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "array",
              "items": {
                "type": "string"
              }
            }
          ]
        },
        "opts": {
          "type": "object",
          "additionalProperties": {
            "$ref": "#/definitions/ValueItem"
          }
        }
      },
      "required": [
        "supertype",
        "type",
        "function",
        "opts"
      ],
      "additionalProperties": false
    },
    "DataItem": {
      "type": "object",
      "properties": {
        "supertype": {
          "type": "string",
          "const": "value"
        },
        "type": {
          "type": "string",
          "const": "data"
        },
        "source": {
          "type": "string"
        },
        "default": {
          "$ref": "#/definitions/ValueItem"
        }
      },
      "required": [
        "supertype",
        "type",
        "source"
      ],
      "additionalProperties": false
    },
    "FormDataItem": {
      "type": "object",
      "properties": {
        "supertype": {
          "type": "string",
          "const": "value"
        },
        "type": {
          "type": "string",
          "const": "form-data"
        },
        "path": {
          "type": "string"
        },
        "default": {
          "$ref": "#/definitions/ValueItem"
        }
      },
      "required": [
        "supertype",
        "type",
        "path"
      ],
      "additionalProperties": false
    },
    "AvatarType": {
      "anyOf": [
        {
          "type": "object",
          "properties": {
            "type": {
              "type": "string",
              "const": "text"
            },
            "text": {
              "$ref": "#/definitions/ValueItem"
            }
          },
          "required": [
            "type",
            "text"
          ],
          "additionalProperties": false
        },
        {
          "type": "object",
          "properties": {
            "type": {
              "type": "string",
              "const": "icon"
            },
            "icon": {
              "$ref": "#/definitions/IconType"
            }
          },
          "required": [
            "type",
            "icon"
          ],
          "additionalProperties": false
        },
        {
          "type": "object",
          "properties": {
            "type": {
              "type": "string",
              "const": "image"
            },
            "source": {
              "$ref": "#/definitions/ValueItem"
            },
            "alt": {
              "$ref": "#/definitions/ValueItem"
            }
          },
          "required": [
            "type",
            "source",
            "alt"
          ],
          "additionalProperties": false
        }
      ]
    },
    "IconType": {
      "anyOf": [
        {
          "type": "object",
          "properties": {
            "family": {
              "$ref": "#/definitions/IconFamily"
            },
            "name": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "$ref": "#/definitions/ValueItem"
                }
              ]
            }
          },
          "required": [
            "family",
            "name"
          ],
          "additionalProperties": false
        },
        {
          "$ref": "#/definitions/ValueItem"
        }
      ]
    },
    "IconFamily": {
      "type": "string",
      "enum": [
        "md",
        "gi"
      ]
    },
    "AllRenderItems": {
      "anyOf": [
        {
          "$ref": "#/definitions/RenderGroupItem"
        },
        {
          "$ref": "#/definitions/RenderTextItem"
        },
        {
          "$ref": "#/definitions/RenderDividerItem"
        },
        {
          "$ref": "#/definitions/RenderChipItem"
        },
        {
          "$ref": "#/definitions/RenderListItem"
        },
        {
          "$ref": "#/definitions/RenderStackItem"
        },
        {
          "$ref": "#/definitions/RenderTableItem"
        },
        {
          "$ref": "#/definitions/RenderTableRowItem"
        },
        {
          "$ref": "#/definitions/RenderMarkdownItem"
        },
        {
          "$ref": "#/definitions/RenderAbsoluteContainerItem"
        },
        {
          "$ref": "#/definitions/RenderAbsoluteItem"
        },
        {
          "$ref": "#/definitions/RenderAccordionItem"
        },
        {
          "$ref": "#/definitions/RenderCardItem"
        },
        {
          "$ref": "#/definitions/RenderSegmentItem"
        },
        {
          "$ref": "#/definitions/RenderMasonryItem"
        },
        {
          "$ref": "#/definitions/RenderSimpleFieldItem"
        },
        {
          "$ref": "#/definitions/RenderTextFieldItem"
        },
        {
          "$ref": "#/definitions/RenderSelectFieldItem"
        },
        {
          "$ref": "#/definitions/RenderSelectFieldOptionItem"
        },
        {
          "$ref": "#/definitions/RenderAutocompleteFieldItem"
        },
        {
          "$ref": "#/definitions/RenderSwitchItem"
        },
        {
          "$ref": "#/definitions/RenderToggleButtonItem"
        },
        {
          "$ref": "#/definitions/RenderTextEditor"
        },
        {
          "$ref": "#/definitions/RenderContentSelector"
        }
      ]
    },
    "RenderGroupItem": {
      "type": "object",
      "properties": {
        "supertype": {
          "type": "string",
          "const": "render"
        },
        "type": {
          "type": "string",
          "const": "group"
        },
        "conditionalRender": {
          "$ref": "#/definitions/ParsedFunction"
        },
        "children": {
          "anyOf": [
            {
              "type": "array",
              "items": {
                "$ref": "#/definitions/AllRenderItems"
              }
            },
            {
              "$ref": "#/definitions/AllSourceItems"
            }
          ]
        }
      },
      "required": [
        "supertype",
        "type"
      ],
      "additionalProperties": false
    },
    "ParsedFunction": {
      "type": "object",
      "properties": {
        "function": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "array",
              "items": {
                "type": "string"
              }
            }
          ]
        },
        "opts": {
          "type": "object",
          "additionalProperties": {
            "anyOf": [
              {
                "$ref": "#/definitions/Literal"
              },
              {
                "$ref": "#/definitions/ValueItem"
              }
            ]
          }
        }
      },
      "required": [
        "function",
        "opts"
      ],
      "additionalProperties": false
    },
    "AllSourceItems": {
      "anyOf": [
        {
          "$ref": "#/definitions/ListSourceItem%3Cany%3E"
        },
        {
          "$ref": "#/definitions/GeneratorSourceItem%3Cany%3E"
        }
      ]
    },
    "ListSourceItem<any>": {
      "type": "object",
      "properties": {
        "supertype": {
          "type": "string",
          "const": "source"
        },
        "type": {
          "type": "string",
          "const": "list"
        },
        "source": {
          "type": "string",
          "description": "Dot path to an array value in the data (ie. i.am.a.path.to.an.array)"
        },
        "renderer": {
          "$ref": "#/definitions/AllRenderItems"
        },
        "conditionalRender": {
          "$ref": "#/definitions/ParsedFunction"
        }
      },
      "required": [
        "supertype",
        "type",
        "source",
        "renderer"
      ],
      "additionalProperties": false
    },
    "GeneratorSourceItem<any>": {
      "type": "object",
      "properties": {
        "supertype": {
          "type": "string",
          "const": "source"
        },
        "type": {
          "type": "string",
          "const": "generator"
        },
        "function": {
          "$ref": "#/definitions/ParsedFunction",
          "description": "A function that takes data and outputs an array of {[key: string]: ValueItem | {...}}"
        },
        "renderer": {
          "$ref": "#/definitions/AllRenderItems"
        },
        "conditionalRender": {
          "$ref": "#/definitions/ParsedFunction"
        }
      },
      "required": [
        "supertype",
        "type",
        "function",
        "renderer"
      ],
      "additionalProperties": false
    },
    "RenderTextItem": {
      "type": "object",
      "properties": {
        "supertype": {
          "type": "string",
          "const": "render"
        },
        "type": {
          "type": "string",
          "const": "text"
        },
        "text": {
          "$ref": "#/definitions/ValueItem"
        },
        "textType": {
          "type": "string",
          "enum": [
            "body1",
            "body2",
            "h1",
            "h2",
            "h3",
            "h4",
            "h5",
            "h6",
            "subtitle1",
            "subtitle2",
            "raw"
          ]
        },
        "style": {
          "type": "array",
          "items": {
            "type": "string",
            "enum": [
              "italic",
              "bold",
              "strikethrough",
              "underline"
            ]
          }
        },
        "conditionalRender": {
          "$ref": "#/definitions/ParsedFunction"
        }
      },
      "required": [
        "supertype",
        "type"
      ],
      "additionalProperties": false
    },
    "RenderDividerItem": {
      "type": "object",
      "properties": {
        "supertype": {
          "type": "string",
          "const": "render"
        },
        "type": {
          "type": "string",
          "const": "divider"
        },
        "child": {
          "$ref": "#/definitions/AllRenderItems"
        },
        "variant": {
          "type": "string",
          "enum": [
            "full",
            "inset",
            "middle"
          ]
        },
        "conditionalRender": {
          "$ref": "#/definitions/ParsedFunction"
        }
      },
      "required": [
        "supertype",
        "type"
      ],
      "additionalProperties": false
    },
    "RenderChipItem": {
      "type": "object",
      "properties": {
        "supertype": {
          "type": "string",
          "const": "render"
        },
        "type": {
          "type": "string",
          "const": "chip"
        },
        "conditionalRender": {
          "$ref": "#/definitions/ParsedFunction"
        },
        "text": {
          "$ref": "#/definitions/ValueItem"
        },
        "filled": {
          "type": "boolean"
        },
        "avatar": {
          "$ref": "#/definitions/AvatarType"
        }
      },
      "required": [
        "supertype",
        "type"
      ],
      "additionalProperties": false
    },
    "RenderListItem": {
      "type": "object",
      "properties": {
        "supertype": {
          "type": "string",
          "const": "render"
        },
        "type": {
          "type": "string",
          "const": "list"
        },
        "conditionalRender": {
          "$ref": "#/definitions/ParsedFunction"
        },
        "itemMarkers": {
          "anyOf": [
            {
              "type": "object",
              "properties": {
                "ordered": {
                  "type": "boolean",
                  "const": true
                },
                "style": {
                  "type": "string",
                  "enum": [
                    "armenian",
                    "cjk-ideographic",
                    "decimal",
                    "decimal-leading-zero",
                    "georgian",
                    "hebrew",
                    "hiragana",
                    "hiragana-iroha",
                    "katakana",
                    "katakana-iroha",
                    "lower-alpha",
                    "lower-greek",
                    "lower-latin",
                    "lower-roman",
                    "upper-alpha",
                    "upper-greek",
                    "upper-latin",
                    "upper-roman"
                  ]
                }
              },
              "required": [
                "ordered",
                "style"
              ],
              "additionalProperties": false
            },
            {
              "type": "object",
              "properties": {
                "ordered": {
                  "type": "boolean",
                  "const": false
                },
                "style": {
                  "type": "string",
                  "enum": [
                    "circle",
                    "disc",
                    "square"
                  ]
                }
              },
              "required": [
                "ordered",
                "style"
              ],
              "additionalProperties": false
            }
          ]
        },
        "children": {
          "anyOf": [
            {
              "type": "array",
              "items": {
                "$ref": "#/definitions/AllRenderItems"
              }
            },
            {
              "$ref": "#/definitions/AllSourceItems"
            }
          ]
        }
      },
      "required": [
        "supertype",
        "type"
      ],
      "additionalProperties": false
    },
    "RenderStackItem": {
      "type": "object",
      "properties": {
        "supertype": {
          "type": "string",
          "const": "render"
        },
        "type": {
          "type": "string",
          "const": "stack"
        },
        "conditionalRender": {
          "$ref": "#/definitions/ParsedFunction"
        },
        "direction": {
          "type": "string",
          "enum": [
            "horizontal",
            "vertical"
          ]
        },
        "spacing": {
          "type": "number"
        },
        "children": {
          "anyOf": [
            {
              "type": "array",
              "items": {
                "$ref": "#/definitions/AllRenderItems"
              }
            },
            {
              "$ref": "#/definitions/AllSourceItems"
            }
          ]
        }
      },
      "required": [
        "supertype",
        "type"
      ],
      "additionalProperties": false
    },
    "RenderTableItem": {
      "type": "object",
      "properties": {
        "supertype": {
          "type": "string",
          "const": "render"
        },
        "type": {
          "type": "string",
          "const": "table"
        },
        "conditionalRender": {
          "$ref": "#/definitions/ParsedFunction"
        },
        "title": {
          "$ref": "#/definitions/ValueItem"
        },
        "headers": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/ValueItem"
          }
        },
        "children": {
          "anyOf": [
            {
              "type": "array",
              "items": {
                "$ref": "#/definitions/RenderTableRowItem"
              }
            },
            {
              "$ref": "#/definitions/AllSourceItems%3CRenderTableRowItem%3E"
            }
          ]
        }
      },
      "required": [
        "supertype",
        "type"
      ],
      "additionalProperties": false
    },
    "RenderTableRowItem": {
      "type": "object",
      "properties": {
        "supertype": {
          "type": "string",
          "const": "render"
        },
        "type": {
          "type": "string",
          "const": "tableRow"
        },
        "conditionalRender": {
          "$ref": "#/definitions/ParsedFunction"
        },
