{
    "supertype": "render",
    "type": "group",
    "children": [
        {
            "supertype": "render",
            "type": "text-field",
            "fieldId": "name",
            "placeholder": "Name",
            "icon": {
                "type": {
                    "type": "icon",
                    "icon": {
                        "family": "md",
                        "name": "MdPerson"
                    }
                },
                "position": "start"
            }
        },
        {
            "supertype": "render",
            "type": "text-field",
            "fieldId": "age",
            "placeholder": "Age",
            "icon": {
                "type": {
                    "type": "icon",
                    "icon": {
                        "family": "md",
                        "name": "MdWatchLater"
                    }
                },
                "position": "start"
            },
            "numerical": true
        },
        {
            "supertype": "render",
            "type": "text",
            "text": {
                "type": "text",
                "supertype": "value",
                "content": "Hi, {{name}}! You are {{age}} years old.",
                "substitutions": {
                    "name": "$form:name",
                    "age": "$form:age"
                }
            }
        }
    ]
}
